comment '--###-- EDITORS --###--'
 bool ' awk' CONFIG_USER_BUSYBOX_AWK n
 if [ "$CONFIG_USER_BUSYBOX_AWK" = "y" ]; then
   bool '   Enable math functions (requires libm)' CONFIG_USER_BUSYBOX_FEATURE_AWK_MATH y
 fi
 bool ' ed' CONFIG_USER_BUSYBOX_ED n
 bool ' patch' CONFIG_USER_BUSYBOX_PATCH n
 bool ' sed' CONFIG_USER_BUSYBOX_SED n
 bool ' vi' CONFIG_USER_BUSYBOX_VI n
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Enable ":" colon commands (no "ex" mode)' CONFIG_USER_BUSYBOX_FEATURE_VI_COLON y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Enable yank/put commands and mark cmds' CONFIG_USER_BUSYBOX_FEATURE_VI_YANKMARK y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Enable search and replace cmds' CONFIG_USER_BUSYBOX_FEATURE_VI_SEARCH y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Catch signals' CONFIG_USER_BUSYBOX_FEATURE_VI_USE_SIGNALS y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Remember previous cmd and "." cmd' CONFIG_USER_BUSYBOX_FEATURE_VI_DOT_CMD y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Enable -R option and "view" mode' CONFIG_USER_BUSYBOX_FEATURE_VI_READONLY y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Enable set-able options, ai ic showmatch' CONFIG_USER_BUSYBOX_FEATURE_VI_SETOPTS y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Support for :set' CONFIG_USER_BUSYBOX_FEATURE_VI_SET y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Handle window resize' CONFIG_USER_BUSYBOX_FEATURE_VI_WIN_RESIZE y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" ]; then
   bool '   Optimize cursor movement' CONFIG_USER_BUSYBOX_FEATURE_VI_OPTIMIZE_CURSOR y
 fi
 if [ "$CONFIG_USER_BUSYBOX_VI" = "y" -o "$CONFIG_USER_BUSYBOX_AWK" = "y" ]; then
   bool '   Allow vi and awk to execute shell commands' CONFIG_USER_BUSYBOX_FEATURE_ALLOW_EXEC y
 fi

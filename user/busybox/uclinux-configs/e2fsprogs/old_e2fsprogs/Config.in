 bool ' fsck' CONFIG_USER_BUSYBOX_FSCK n
 bool ' lsattr' CONFIG_USER_BUSYBOX_LSATTR n
 bool ' mke2fs' CONFIG_USER_BUSYBOX_MKE2FS n

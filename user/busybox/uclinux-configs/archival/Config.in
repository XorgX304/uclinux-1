comment '--###-- ARCHIVAL UTILITIES --###--'
 bool ' ar' CONFIG_USER_BUSYBOX_AR n
 if [ "$CONFIG_USER_BUSYBOX_AR" = "y" ]; then
   bool '   Enable support for long filenames (not need for debs)' CONFIG_USER_BUSYBOX_FEATURE_AR_LONG_FILENAMES n
 fi
 bool ' bunzip2' CONFIG_USER_BUSYBOX_BUNZIP2 n
 bool ' cpio' CONFIG_USER_BUSYBOX_CPIO n
 bool ' dpkg' CONFIG_USER_BUSYBOX_DPKG n
 bool ' dpkg_deb' CONFIG_USER_BUSYBOX_DPKG_DEB n
 if [ "$CONFIG_USER_BUSYBOX_DPKG_DEB" = "y" ]; then
   bool '   extract only (-x)' CONFIG_USER_BUSYBOX_FEATURE_DPKG_DEB_EXTRACT_ONLY n
 fi
 bool ' gunzip' CONFIG_USER_BUSYBOX_GUNZIP n
 if [ "$CONFIG_USER_BUSYBOX_GUNZIP" = "y" ]; then
   bool '   Uncompress support' CONFIG_USER_BUSYBOX_FEATURE_GUNZIP_UNCOMPRESS n
 fi
 bool ' gzip' CONFIG_USER_BUSYBOX_GZIP n
 bool ' rpm2cpio' CONFIG_USER_BUSYBOX_RPM2CPIO n
 bool ' rpm' CONFIG_USER_BUSYBOX_RPM n
 bool ' tar' CONFIG_USER_BUSYBOX_TAR n
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable archive creation' CONFIG_USER_BUSYBOX_FEATURE_TAR_CREATE y
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable -j option to handle .tar.bz2 files' CONFIG_USER_BUSYBOX_FEATURE_TAR_BZIP2 n
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable -a option to handle .tar.lzma files' CONFIG_USER_BUSYBOX_FEATURE_TAR_LZMA n
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable -X (exclude from) and -T (include from) options)' CONFIG_USER_BUSYBOX_FEATURE_TAR_FROM n
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable -z option' CONFIG_USER_BUSYBOX_FEATURE_TAR_GZIP y
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable -Z option' CONFIG_USER_BUSYBOX_FEATURE_TAR_COMPRESS n
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable support for old tar header format' CONFIG_USER_BUSYBOX_FEATURE_TAR_OLDGNU_COMPATIBILITY N
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable support for some GNU tar extensions' CONFIG_USER_BUSYBOX_FEATURE_TAR_GNU_EXTENSIONS y
 fi
 if [ "$CONFIG_USER_BUSYBOX_TAR" = "y" -a "$CONFIG_USER_BUSYBOX_GETOPT_LONG" = "y" ]; then
   bool '   Enable long options' CONFIG_USER_BUSYBOX_FEATURE_TAR_LONG_OPTIONS n
 fi
 bool ' uncompress' CONFIG_USER_BUSYBOX_UNCOMPRESS n
 bool ' unlzma' CONFIG_USER_BUSYBOX_UNLZMA n
 if [ "$CONFIG_USER_BUSYBOX_UNLZMA" = "y" ]; then
   bool '   Optimze unlzma for speed' CONFIG_USER_BUSYBOX_FEATURE_LZMA_FAST n
 fi
 bool ' unzip' CONFIG_USER_BUSYBOX_UNZIP n
 if [ "$CONFIG_USER_BUSYBOX_CPIO" = "y" -o "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   comment ' Common options for cpio and tar'
 fi
 if [ "$CONFIG_USER_BUSYBOX_CPIO" = "y" -o "$CONFIG_USER_BUSYBOX_TAR" = "y" ]; then
   bool '   Enable tape drive support' CONFIG_USER_BUSYBOX_FEATURE_UNARCHIVE_TAPE n
 fi
 if [ "$CONFIG_USER_BUSYBOX_DPKG" = "y" -o "$CONFIG_USER_BUSYBOX_DPKG_DEB" = "y" ]; then
   comment ' Common options for dpkg and dpkg_deb'
 fi
 if [ "$CONFIG_USER_BUSYBOX_DPKG" = "y" -o "$CONFIG_USER_BUSYBOX_DPKG_DEB" = "y" ]; then
   bool '   gzip debian packages (normal)' CONFIG_USER_BUSYBOX_FEATURE_DEB_TAR_GZ y
 fi
 if [ "$CONFIG_USER_BUSYBOX_DPKG" = "y" -o "$CONFIG_USER_BUSYBOX_DPKG_DEB" = "y" ]; then
   bool '   bzip2 debian packages' CONFIG_USER_BUSYBOX_FEATURE_DEB_TAR_BZ2 n
 fi
 if [ "$CONFIG_USER_BUSYBOX_DPKG" = "y" -o "$CONFIG_USER_BUSYBOX_DPKG_DEB" = "y" ]; then
   bool '   lzma debian packages' CONFIG_USER_BUSYBOX_FEATURE_DEB_TAR_LZMA n
 fi
